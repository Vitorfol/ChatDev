                    int dir = -1;
                    if (e.getKeyCode() == KeyEvent.VK_UP) dir = 0;
                    if (e.getKeyCode() == KeyEvent.VK_RIGHT) dir = 1;
                    if (e.getKeyCode() == KeyEvent.VK_DOWN) dir = 2;
                    if (e.getKeyCode() == KeyEvent.VK_LEFT) dir = 3;
