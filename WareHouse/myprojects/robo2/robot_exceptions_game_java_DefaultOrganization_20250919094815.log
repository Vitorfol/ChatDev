                    if (moved) log(r.getName() + " moved (valid).");
                } catch (MovimentoInvalidoException ex) {
                    r.incrementInvalid();
                    log(r.getName() + " invalid: " + ex.getMessage());
                }
            }
        }
        board.checkGameState();
        gridPanel.repaint();
        logStats();
    }
    private void stopSimulation() {
        if (simThread != null && simThread.isAlive()) {
