        for (Robo r : new ArrayList<>(board.getRobots())) {
            sb.append(String.format("%s - Pos:%s Alive:%s Food:%s Valid:%d Invalid:%d\n",
                    r.getName(), r.getPosition(), r.isAlive(), r.foundFood(), r.getValidMoves(), r.getInvalidMoves()));
