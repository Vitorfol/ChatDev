            while (!board.isFinished()) {
                delay = delaySlider.getValue();
                int mode = modeBox.getSelectedIndex();
                if (mode == 0) {
                    // user control â€” nothing to automate
                } else if (mode == 1) {
                    // single RoboInteligente random moves attempts
