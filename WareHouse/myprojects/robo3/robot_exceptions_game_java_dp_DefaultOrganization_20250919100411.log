                        } catch (MovimentoInvalidoException ex) {
                            // no possible moves left for this robot
                            break;
                        }
                    }
                }).start();
            }
            // Update statuses periodically and detect termination
            new Timer(300, e -> {
                boolean anyFound = false;
                boolean anyAlive = false;
                for (int i = 0; i < robotCount; i++) {
